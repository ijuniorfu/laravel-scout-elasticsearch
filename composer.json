{
    "name": "ijuniorfu/laravel-scout-elasticsearch",
    "description": "Elastic Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch", "elastic"],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "laravel/scout": "^5.0|^6.0|^7.0",
        "elasticsearch/elasticsearch": "^6.7"
    },
     "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Junior\\ScoutEngines\\Elasticsearch\\": "src/"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^6.7)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Junior\\ScoutEngines\\Elasticsearch\\ElasticsearchProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
